<!DOCTYPE html>
<html>
    <head>
        <title>Weather App</title>
        <link href="App.css" type="text/css" rel="stylesheet"/>
        <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    </head>
    <body>
        <header id="header">
            <img id="logo" src="https://android.giveawayoftheday.com/wp-content/plugins/gotd_googleplay_plugin/images/2018/01/com.rainystudio.pixelnougaticonpack_app_icon_1516009340.png">
            <input id="Searchbox" placeholder="Search" autocomplete="on" name="City">
            <button id="sub">Submit</button>
            <button id="sub">Settings</button>
            <div id="msg"></div>
        </header>
    
        <div id="mapbox" class="w3-card"></div>
       
        <div id="home"  class="w3-card">
            <div id="homeloc" class="info w3-center w3-xxlarge"></div>
            <div id="temp" class="info w3-center w3-xlarge"></div>
            <div id="summary" class="info w3-center w3-xlarge"></div>
        </div>

        <div id="attract" class="w3-card">
            <h2 class="el-head">List of Attractions</h2>
        </div>

        <div id="weather" class="w3-card">
            <h2 class="el-head">5-Day</h2>
            <div id="w_day1" class="w_days">
                <div id="w_icon1"></div>
                <div id="w_desc1"></div>
                <div id="w_temp1"></div>
            </div>
            <div id="w_day2" class="w_days">
                <div id="w_icon2"></div>
                <div id="w_desc2"></div>
                <div id="w_temp2"></div>
            </div>
            <div id="w_day3" class="w_days">
                <div id="w_icon3"></div>
                <div id="w_desc3"></div>
                <div id="w_temp3"></div>
            </div>
            <div id="w_day4" class="w_days">
                <div id="w_icon4"></div>
                <div id="w_desc4"></div>
                <div id="w_temp4"></div>
            </div>
            <div id="w_day5" class="w_days">
                <div id="w_icon5"></div>
                <div id="w_desc5"></div>
                <div id="w_temp5"></div>
            </div>
        </div>

        <div id="news" class="w3-card">
            <h2 class="el-head">News</h2>
            <div id="slideshow">
                <div class="mySlides fade">
                    <div class="numbertext">1 / 3</div>
                    <img src="img_nature_wide.jpg" style="width:80%">
                    <div>First Thing</div>
                    <div class="text">Caption Text</div>
                </div>

                <div class="mySlides fade">
                    <div class="numbertext">2 / 3</div>
                    <img src="img_fjords_wide.jpg" style="width:80%">
                    <div>Second Thing</div>
                    <div class="text">Caption Two</div>
                </div>

                <div class="mySlides fade">
                    <div class="numbertext">3 / 3</div>
                    <img src="img_mountains_wide.jpg" style="width:80%">
                    <div>Third Thing</div>
                    <div class="text">Caption Three</div>
                </div>
            </div>
        </div>

        <div style="display:none;"> 
            <p id="lat">{{latitude}}</p>
            <p id="lng">{{longitude}}</p>
        </div>

        
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script src="App.js"></script>
        <!-- api key needed below -->
        <script src="https://maps.googleapis.com/maps/api/js?key={{apikey}}&libraries=places&callback=theMap"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    </body>
</html>